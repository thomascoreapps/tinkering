host *.coreapps.net
  ForwardAgent yes
  ServerAliveInterval 60
  GSSAPIAuthentication no
  GSSAPIKeyExchange no
  ControlMaster auto
  ControlPath ~/.ssh/master-%r@%h:%p
  ControlPersist 5m

host *.core-apps.com
  ForwardAgent yes
  ServerAliveInterval 60
  GSSAPIAuthentication no
  GSSAPIKeyExchange no
  ControlMaster auto
  ControlPath ~/.ssh/master-%r@%h:%p
  ControlPersist 5m

host *.core-apps.xyz
  ForwardAgent yes
  ServerAliveInterval 60
  GSSAPIAuthentication no
  GSSAPIKeyExchange no
  ControlMaster auto
  ControlPath ~/.ssh/master-%r@%h:%p
  ControlPersist 5m

host *
  ForwardAgent yes
  ServerAliveInterval 60
  GSSAPIAuthentication no
  GSSAPIKeyExchange no
  ControlMaster auto
  ControlPath ~/.ssh/master-%r@%h:%p
  ControlPersist 5m


